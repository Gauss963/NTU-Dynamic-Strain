material elastic [
  name = pmma
  rho = 1180
  E   = 3.0e9
  nu  = 0.35
]

contact_detector [
  type = explicit
  master = friction_master
  slave  = friction_slave
  projection_tolerance = 1e-10
  max_iterations       = 200
  extension_tolerance  = 1e-6
]

contact_resolution penalty_linear [
  name = friction_slave
  mu = 0.60
  epsilon_n = 1e10
  epsilon_t = 1e9
  is_master_deformable = true
]