model solid_mechanics_model_cohesive [

  cohesive_inserter [
     cohesive_zones = [friction_master, friction_slave]
   ]

  material elastic [
    name = moving-block
    rho = 1.18e-6
    E   = 3000
    nu  = 0.35
  ]

  material elastic [
    name = stationary-block
    rho = 1.18e-6
    E   = 3000
    nu  = 0.35
  ]

  material cohesive_linear [
    name = interface_mat
    beta = 0
    G_c = 0.2
    sigma_c = 50
  ]
]